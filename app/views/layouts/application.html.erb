<!DOCTYPE html>
<html>
<head>
  <title>ChicagoCommunitiesWebclient</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body id="skin-blur-ocean">

<header id="header" class="media">
  <a href="" id="menu-toggle"></a> 
  <a class="logo pull-left" href="index.html">Chicago City Data</a>
  
  <div class="media-body">
      <div class="media" id="top-menu">
          <div class="pull-left tm-icon">
          </div>
          <div class="pull-left tm-icon">
          </div>
          
          

          <div id="time" class="pull-right">
              <span id="hours"></span>
              :
              <span id="min"></span>
              :
              <span id="sec"></span>
          </div>

          <div class="media-body">
              <input type="text" class="main-search">
          </div>
      </div>
  </div>
</header>

<div class="clearfix"></div>

<section id="main" class="p-relative" role="main">
    
    <aside id="sidebar">
                    
        <!-- Sidbar Widgets -->
        <div class="side-widgets overflow">
            <!-- Profile Menu -->
            <div class="text-center s-widget m-b-25 dropdown" id="profile-menu">
                <a href="" data-toggle="dropdown">
                    <img class="profile-pic animated" src="/assets/neighborhood.jpg" alt="">
                </a>
                <ul class="dropdown-menu profile-menu">
                    <li><a href="">My Profile</a> <i class="icon left">&#61903;</i><i class="icon right">&#61815;</i></li>
                    <li><a href="">Messages</a> <i class="icon left">&#61903;</i><i class="icon right">&#61815;</i></li>
                    <li><a href="">Settings</a> <i class="icon left">&#61903;</i><i class="icon right">&#61815;</i></li>
                    <li><a href="">Sign Out</a> <i class="icon left">&#61903;</i><i class="icon right">&#61815;</i></li>
                </ul>
                <h4 class="m-0">Chicago Communities</h4>
            </div>
            
            
        </div>
        
        <!-- Side Menu -->
        <ul class="list-unstyled side-menu">
            <li class="active">
                <a class="sa-side-home" href="/communities">
                    <span class="menu-item">Communities</span>
                </a>
            </li>
            <li>
                <a class="sa-side-table" href="/communities">
                    <span class="menu-item">Tables</span>
                </a>
                <ul class="list-unstyled menu-item">
                    <li><a href="/socio_economic_indicators">SocioEconomic Factors</a></li>
                    <li><a href="/life_expectancies">Life Expectancies</a></li>
                    <li><a href="/vacant_properties">Vacant Properties</a></li>
                </ul>
            </li>
            <li>
                <a class="sa-side-chart" href="charts.html">
                    <span class="menu-item">Charts</span>
                </a>
            </li>
            <li class="dropdown">
                <a class="sa-side-page" href="">
                    <span class="menu-item">Pages</span>
                </a>
                <ul class="list-unstyled menu-item">
                    <li><a href="list-view.html">List View</a></li>
                    <li><a href="profile-page.html">Profile Page</a></li>
                    <li><a href="messages.html">Messages</a></li>
                    <li><a href="login.html">Login</a></li>
                    <li><a href="404.html">404 Error</a></li>
                </ul>
            </li>
        </ul>

    </aside>
<section id="content" class="container">
  <%= yield %>
</section>

</body>
</html>
